<!-- 商品 - start -->
<div class="justify-center items-center pt-10 pb-8 bg-white">
  <div class="flex-col justify-start items-center flex">
    <div class="text-center text-dark-green text-lg md:text-xl font-bold font-['Noto Sans JP'] tracking-wide">
      おすすめ商品
    </div>
    <div class="w-[80px] md:w-[140px] h-px bg-light-green">
    </div>
    <div class="pt-[4px] text-center text-light-green text-xs md:text-base font-['Inter'] tracking-wide">
      Products
    </div>
  </div>

  <%# MEMO:テキストのコントラスト比が強すぎるので優しめにする必要あり %>
  <div class="flex xl:justify-center overflow-scroll">
    <% @products.each do |product| %>
      <div class="pl-6 md:pl-6 pt-6">
        <div class="w-[166px] h-[270px] md:w-[260px] md:h-[360px] bg-gray-100 shadow-md rounded-lg">
          <%= link_to product_path(product) do %>
            <%= image_tag product.image, class: "object-cover rounded-t-lg" %>
            <div class="px-5 pb-2">
              <p class="text-sm md:text-md font-['Noto Sans JP'] font-semibold tracking-tight text-dark-green mt-4 mb-4"><%= product.name %></p>
              <div class="flex justify-between items-center">
                <span class="text-sm md:text-md font-['Noto Sans'] font-bold text-dark-green"><%= number_to_currency(product.price, unit: "¥", strip_insignificant_zeros: true) %></span>
                <% if product.stock > 0 %>
                  <span class="bg-blue-100 text-blue-800 text-[10px] md:text-xs font-['Noto Sans JP'] font-semibold p-2 rounded">在庫あり</span>
                <% else %>
                  <span class="bg-red-100 text-red-800 text-[10px] md:text-xs font-['Noto Sans JP'] font-semibold p-2 rounded">在庫なし</span>
                <% end %>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>

<!-- 商品 buttun - start -->
  <div class="flex justify-center mt-2">
    <div class='mt-3'>
      <%= link_to products_path, class:"group inline-flex items-center justify-center rounded-full bg-light-green hover:bg-[#acae98] focus:ring-gray-100 focus:ring-gray-300" do %>
        <span class="font-['Noto Sans JP'] md:font-medium text-base text-white px-4 py-2 duration-75 ease-in group-hover:bg-opacity-0">
          すべてを見る
        </span>
      <% end %>
    </div>
  </div>
<!-- 商品 buttun - end -->


</div>
